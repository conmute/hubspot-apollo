{
  "env": {
    "production": {
      "plugins": [
				"emotion",
				["module-resolver", {
					"root": ["./"],
					"alias": { "~": "./" }
				}]
			]
    },
    "development": {
			"plugins": [
				["emotion", { "sourceMap": true }],
				["module-resolver", {
					"root": ["./"],
					"alias": { "~": "./" }
				}]
			]
    }
	},
	"presets": ["next/babel"]
}
